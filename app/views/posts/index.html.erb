<h1 id="page-title"> Stuff I've written </h1>
  <div id="posts-wrapper" class='skinny-wrapper'>
    <% @posts.each do |post| %>
      <div class="post">
        <p class="date"><%= post.created_at.strftime('%A, %B %d') %></p>
        <h2><a href="#"><%= link_to post.title, post %></a></h2>
        <hr>
      </div>
    <% end %>
  </div>

  <div class="pagination">
    <span class='previous_page disabled'> <- Previous </span>
    <em class="current"> 1 </em>
    <a rel='next' hrerf='#'> 2 </a>
    <a class='next_page' href='#'> Next -> </a>
  </div>
